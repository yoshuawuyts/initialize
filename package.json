{
  "name": "initialize",
  "version": "7.3.1",
  "description": "Generate a fresh package",
  "main": "index.js",
  "bin": "./bin/initialize",
  "scripts": {
    "test": "node bin/initialize -d ./tmp",
    "test:random": "node bin/initialize -d ./tmp/\"$RANDOM\"",
    "clean": "rm -rf ./tmp",
    "deps": "dependency-check . && dependency-check . --extra --no-dev"
  },
  "repository": "yoshuawuyts/initialize",
  "preferGlobal": true,
  "keywords": [
    "new",
    "project",
    "cli",
    "shell"
  ],
  "license": "MIT",
  "dependencies": {
    "cliclopts": "^1.1.1",
    "dates-of-today": "^1.0.0",
    "git-init": "^1.0.0",
    "initialize-engine": "^1.1.0",
    "inquirer": "^0.9.0",
    "minimist": "^1.1.3",
    "mkdirp": "^0.4.0",
    "object-assign": "^3.0.0",
    "rc": "^1.1.0",
    "variable-name": "0.0.1"
  },
  "devDependencies": {
    "dependency-check": "^2.5.0",
    "standard": "^5.3.1"
  }
}
